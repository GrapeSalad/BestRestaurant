@{
  Layout="Shared/_Layout.cshtml";
}

<h3>Cuisines</h3>
<ul>
  @if (Model.Count !=0)
  {
    @foreach(var cuisine in Model)
    {
      <li><a href="/cuisines/@cuisine.GetId()">@cuisine.GetType()</a></li>
    }
  }
  else
  {
    <p>AINT NOTHIN HERE</p>
  }
</ul>
<h4><a href ="cuisines/new">Add a new cuisine!</a></h4>
<h4><a href="/">Return home</a></h4>

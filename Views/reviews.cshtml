@{
  Layout="Shared/_Layout.cshtml";
}

<h3>Reviews</h3>
<ul>
  @if (Model.Count !=0)
  {
    @foreach(var review in Model)
    {
      <li><a href="/reviews/@review.GetId()">@review.GetUserName(), @review.GetScore()</a></li>
    }
  }
  else
  {
    <p>AINT NOTHIN HERE</p>
  }
</ul>
<h4><a href ="reviews/new">Add a new review!</a></h4>
<h4><a href="/">Return home</a></h4>

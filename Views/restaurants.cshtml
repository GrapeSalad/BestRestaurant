@{
  Layout="Shared/_Layout.cshtml";
}

<h3>Restaurants</h3>
<ul>
  @if (Model.Count !=0)
  {
    @foreach(var restaurants in Model)
    {
      <li><a href="/restaurants/@restaurants.GetId()">@restaurants.GetName()</a></li>
    }
  }
  else
  {
    <p>AINT NOTHIN HERE</p>
  }
</ul>
<h4><a href ="/restaurants/new">Add a new restaurant!</a></h4>
<h4><a href="/">Return home</a></h4>
